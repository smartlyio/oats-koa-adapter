{
  "name": "@smartlyio/oats-koa-adapter",
  "version": "2.3.1",
  "license": "MIT",
  "description": "Koa adapter for Oats",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf ./dist && rm -f ./tmp/*.ts",
    "build": "yarn clean && yarn lint && yarn tsc",
    "prepublish": "yarn build",
    "render": "ts-node render-docs.ts",
    "render:watch": "nodemon -w \"examples/\" -w \"README.template.md\" render-docs.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.10.0",
    "koa": "^2.3.0",
    "koa-body": "^4.0.4",
    "koa-router": "^10.0.0",
    "typescript": "^4.2.0"
  },
  "keywords": [
    "oats",
    "koa",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-runtime": "2.13.2",
    "@types/koa": "2.13.4",
    "@types/koa-router": "7.4.4",
    "@types/lodash": "4.14.171",
    "@types/node": "14.17.5",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  }
}
