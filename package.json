{
  "name": "@smartlyio/oats-koa-adapter",
  "version": "2.1.0",
  "license": "MIT",
  "description": "Koa adapter for Oats",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf ./dist && rm -f ./tmp/*.ts",
    "build": "yarn clean && yarn lint && yarn tsc",
    "prepublish": "yarn build",
    "render": "ts-node render-docs.ts",
    "render:watch": "nodemon -w \"examples/\" -w \"README.template.md\" render-docs.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats.git"
  },
  "peerDependencies": {
    "@smartlyio/oats-runtime": "^2.8.0",
    "koa": "^2.3.0",
    "koa-body": "^4.0.4",
    "koa-router": "^10.0.0",
    "typescript": "^3.6.3"
  },
  "keywords": [
    "oats",
    "koa",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats-runtime": "2.10.0",
    "@types/koa": "2.11.6",
    "@types/koa-router": "7.4.1",
    "@types/lodash": "4.14.168",
    "@types/node": "11.15.20",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.18.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.20.0",
    "nodemon": "2.0.7",
    "eslint-plugin-prettier": "3.3.1",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "3.9.7"
  }
}
